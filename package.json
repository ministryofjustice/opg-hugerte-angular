    "eslint": "^9.39.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "gh-pages": "^6.1.0",
    "hugerte": "^1.0.4",
    "json": "11.0.0",
    "ng-packagr": "^19.2.2",
    "react": "^18.2.0",
